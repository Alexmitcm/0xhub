{
  "name": "@hey/api",
  "version": "0.0.0",
  "private": true,
  "license": "AGPL-3.0",
  "scripts": {
    "build": "prisma generate --schema ./src/prisma/schema.prisma && tsc",
    "codegen": "prisma generate --schema ./src/prisma/schema.prisma",
    "dev": "tsx watch src/index.ts",
    "postinstall": "pnpm codegen",
    "prisma:clean": "prisma migrate reset --schema ./src/prisma/schema.prisma",
    "prisma:format": "prisma format --schema ./src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./src/prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema ./src/prisma/schema.prisma",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --pretty",
    "deploy:render": "pnpm build && pnpm prisma:migrate:deploy && pnpm start:prod"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@apollo/client": "^3.14.0",
    "@aws-sdk/client-sts": "^3.848.0",
    "@guildxyz/sdk": "^2.6.9",
    "@hey/data": "workspace:*",
    "@hey/helpers": "workspace:*",
    "@hey/indexer": "workspace:*",
    "@hono/node-server": "^1.17.1",
    "@hono/zod-validator": "^0.7.2",
    "@lens-chain/sdk": "^1.0.3",
    "@prisma/client": "^6.12.0",
    "@supabase/supabase-js": "^2.53.0",
    "@types/bcrypt": "^6.0.0",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.12.2",
    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.1",
    "hono": "^4.8.9",
    "hono-rate-limiter": "^0.4.2",
    "ioredis": "^5.4.1",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "linkedom": "^0.18.11",
    "pg-promise": "^11.15.0",
    "redis": "^5.6.1",
    "tsx": "^4.20.3",
    "viem": "^2.33.1",
    "ws": "^8.18.0",
    "xmlbuilder2": "^3.1.1",
    "zod": "3.25.75"
  },
  "devDependencies": {
    "@hey/config": "workspace:*",
    "@hey/types": "workspace:*",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.1.0",
    "@types/unzip-stream": "^0.3.4",
    "@types/ws": "^8.18.1",
    "prisma": "^6.12.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
